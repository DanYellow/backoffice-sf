{% block choice_widget %}
  {% spaceless %}
      <div {{ block('widget_container_attributes') }}>
          {% for child in form %}
              {{ form_widget(child) }}
              {{ form_label(child, null, {translation_domain: choice_translation_domain}) }}
          {% endfor %}
          <p>g,krejgehnghergergnjre</p>
      </div>
  {% endspaceless %}
{% endblock %}


{% block project_state_widget %}
  {% spaceless %}
    {% if expanded %}
      {% for child in form %}
        <div class="input-radio-container">
          {{ form_widget(child) }}
          {{ form_label(child) }}
        </div>
      {% endfor %}
    {% else %}
      {# just let the choice widget render the select tag #}
      {{ block('choice_widget') }}
    {% endif %}
  {% endspaceless %}
{% endblock %}
